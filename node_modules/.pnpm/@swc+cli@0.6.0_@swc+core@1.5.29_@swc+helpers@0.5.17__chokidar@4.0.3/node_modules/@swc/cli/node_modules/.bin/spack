#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yongqiwu/code/hthon/org/node_modules/.pnpm/@swc+cli@0.6.0_@swc+core@1.5.29_@swc+helpers@0.5.17__chokidar@4.0.3/node_modules/@swc/cli/bin/node_modules:/Users/yongqiwu/code/hthon/org/node_modules/.pnpm/@swc+cli@0.6.0_@swc+core@1.5.29_@swc+helpers@0.5.17__chokidar@4.0.3/node_modules/@swc/cli/node_modules:/Users/yongqiwu/code/hthon/org/node_modules/.pnpm/@swc+cli@0.6.0_@swc+core@1.5.29_@swc+helpers@0.5.17__chokidar@4.0.3/node_modules/@swc/node_modules:/Users/yongqiwu/code/hthon/org/node_modules/.pnpm/@swc+cli@0.6.0_@swc+core@1.5.29_@swc+helpers@0.5.17__chokidar@4.0.3/node_modules:/Users/yongqiwu/code/hthon/org/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yongqiwu/code/hthon/org/node_modules/.pnpm/@swc+cli@0.6.0_@swc+core@1.5.29_@swc+helpers@0.5.17__chokidar@4.0.3/node_modules/@swc/cli/bin/node_modules:/Users/yongqiwu/code/hthon/org/node_modules/.pnpm/@swc+cli@0.6.0_@swc+core@1.5.29_@swc+helpers@0.5.17__chokidar@4.0.3/node_modules/@swc/cli/node_modules:/Users/yongqiwu/code/hthon/org/node_modules/.pnpm/@swc+cli@0.6.0_@swc+core@1.5.29_@swc+helpers@0.5.17__chokidar@4.0.3/node_modules/@swc/node_modules:/Users/yongqiwu/code/hthon/org/node_modules/.pnpm/@swc+cli@0.6.0_@swc+core@1.5.29_@swc+helpers@0.5.17__chokidar@4.0.3/node_modules:/Users/yongqiwu/code/hthon/org/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/spack.js" "$@"
else
  exec node  "$basedir/../../bin/spack.js" "$@"
fi
