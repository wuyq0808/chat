#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yongqiwu/code/hthon/node_modules/.pnpm/http-server@14.1.1/node_modules/http-server/bin/node_modules:/Users/yongqiwu/code/hthon/node_modules/.pnpm/http-server@14.1.1/node_modules/http-server/node_modules:/Users/yongqiwu/code/hthon/node_modules/.pnpm/http-server@14.1.1/node_modules:/Users/yongqiwu/code/hthon/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yongqiwu/code/hthon/node_modules/.pnpm/http-server@14.1.1/node_modules/http-server/bin/node_modules:/Users/yongqiwu/code/hthon/node_modules/.pnpm/http-server@14.1.1/node_modules/http-server/node_modules:/Users/yongqiwu/code/hthon/node_modules/.pnpm/http-server@14.1.1/node_modules:/Users/yongqiwu/code/hthon/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../http-server/bin/http-server" "$@"
else
  exec node  "$basedir/../http-server/bin/http-server" "$@"
fi
